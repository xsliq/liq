{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "main.js", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/main.js",
      "dest": "/main.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
